{
  "name": "10x-astro-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:e2e": "astro dev --mode test",
    "build": "astro build",
    "start": "node ./dist/server/entry.mjs",
    "preview": "wrangler pages dev ./dist --port 8080",
    "astro": "astro",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "deploy:cf": "npm run build && wrangler pages deploy ./dist --project-name=10xdev",
    "deploy:cf:debug": "npm run deploy:cf && wrangler pages deployment tail --project-name=10xdev"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.5.4",
    "@astrojs/node": "^9.2.2",
    "@astrojs/sitemap": "3.4.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/vite": "4.1.10",
    "@tanstack/react-query": "^5.80.6",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "astro": "5.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "globals": "^16.2.0",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "4.0.17",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.25.62",
    "zod-to-json-schema": "^3.24.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@astrojs/react": "^4.3.0",
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "dotenv": "^16.5.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript-eslint": "8.28.0",
    "vitest": "^3.2.3",
    "wrangler": "^4.20.0"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
